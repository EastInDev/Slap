{
  "singleQuote": true,
  "semi": false,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "importOrder": [
    "^server-only$",
    "^next$",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@/components/ui/.+$",
    "^@/components/.+$",
    "^@/features/.+$",
    "^@/.+$",
    "^\\..+$"
  ],
  "importOrderSeparation": true
}
